{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffffff",
          "properties": {
            "template": "<#C591E8>\u276f</><#69FF94>\u276f</> "
          },
          "style": "plain",
          "type": "text"
        },
        {
          "foreground": "#56B6C2",
          "properties": {
            "mapped_locations": {
              "C:\\Users\\freitag\\source\\repos": "repos",
              "C:\\fastec\\development": "dev"
            },
            "style": "full",
            "template": "{{ .Path }}"
          },
          "style": "plain",
          "type": "path"
        },
        {
          "foreground": "#ffffff",
          "properties": {
            "template": "{{ .Status }}"
          },
          "style": "plain",
          "type": "poshgit"
        },
        {
          "foreground": "#66F68F",
          "properties": {
            "template": " :: {{ .CurrentDate | date .Format }} ",
            "time_format": "15:04"
          },
          "style": "plain",
          "type": "time"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title": true,
  "console_title_style": "folder",
  "version": 1
}
