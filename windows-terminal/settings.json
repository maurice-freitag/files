{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "actions": [
    {
      "command": {
        "action": "splitPane",
        "split": "auto",
        "splitMode": "duplicate"
      },
      "keys": "alt+shift+d"
    },
    {
      "command": {
        "action": "scrollDown"
      },
      "keys": "ctrl+down"
    },
    {
      "command": "scrollUpPage",
      "keys": "ctrl+shift+up"
    },
    {
      "command": "unbound",
      "keys": "ctrl+shift+v"
    },
    {
      "command": {
        "action": "newTab"
      },
      "keys": "ctrl+t"
    },
    {
      "command": {
        "action": "copy"
      },
      "keys": "ctrl+c"
    },
    {
      "command": "paste",
      "keys": "ctrl+v"
    },
    {
      "command": "scrollDownPage",
      "keys": "ctrl+shift+down"
    },
    {
      "command": {
        "action": "closeTab"
      },
      "keys": "ctrl+w"
    },
    {
      "command": {
        "action": "scrollUp"
      },
      "keys": "ctrl+up"
    }
  ],
  "alwaysShowTabs": false,
  "defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
  "initialCols": 150,
  "initialRows": 50,
